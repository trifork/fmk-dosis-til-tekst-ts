{
    "include": [
        "src/main/ts",
    ],
    "exclude": [
        "src/test/**/*.ts"
    ],
    "compilerOptions": {
        "target": "ES6",
        "module": "ESNext",
        "declaration": true,
        "outDir": "target/tsc",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": false
    }
}